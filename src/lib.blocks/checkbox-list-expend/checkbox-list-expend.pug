mixin checkbox-list-expend(params={})
    .checkbox-list-expend(class=params.classes)
        .checkbox-list-expend__title #{params.title}
        .button.button_theme_form-element.checkbox-list-expend__expend-button expand_more
        .checkbox-list-expend__body
            +checkbox-list-items(params.items, 'checkbox-list-expend__checkbox')